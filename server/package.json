{
  "name": "shutfs",
  "version": "0.0.1",
  "description": "Simple stand alone application for share on the web the content of a server folder",
  "main": "index.js",
  "repository": "git@github.com:Zikoel/shutfs.git",
  "author": "Elia Mazzuoli <zikoel@gmail.com>",
  "license": "SEE LICENSE IN LICENZE FILE",
  "private": false,
  "scripts": {
    "dev": "export $(grep -v '^#' .env | xargs) && APP_VERSION=$npm_package_version nodemon --exec ts-node --project tsconfig.json src/start.ts",
    "clean": "rm -rf dist",
    "build": "run-s clean && tsc -p tsconfig.json",
    "image:prod": "docker build .. -f ./docker/Prod.dockerfile -t zikoel/shutfs:v$npm_package_version"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "envalid": "^5.0.0",
    "express": "^4.16.4",
    "fp-ts": "^2.0.5",
    "http-errors": "^1.7.2",
    "io-ts": "^2.0.1",
    "io-ts-types": "^0.5.3",
    "lodash": "^4.17.19",
    "moment": "^2.29.2"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.1.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
    "@typescript-eslint/eslint-plugin": "~2.23.0",
    "@typescript-eslint/parser": "~2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "@types/node": "^11.13.8"
  }
}
